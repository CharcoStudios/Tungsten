<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Version>2.0.0</Version>
    <Authors>Jordan Duerksen</Authors>
    <Company />
    <Product>Tungsten.Net</Product>
    <Copyright>Copyright @ 2017 Jordan Duerksen</Copyright>
    <Description>Tungsten.Net.Standard makes it easy to add tcp sockets to your application. Client and server classes are provided, with and without assymetric encryption.</Description>
    <PackageProjectUrl>https://github.com/mode51/Tungsten</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/mode51/Tungsten/master/Src/Tungsten/Tungsten.ico</PackageIconUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/mode51/Tungsten/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/mode51/Tungsten</RepositoryUrl>
    <PackageTags>Tungsten.Net.Standard Tungsten.Net Tungsten W W.Net C# library tcp sockets client server</PackageTags>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PostBuildEvent></PostBuildEvent>
    <AssemblyName>Tungsten.Net</AssemblyName>
    <RootNamespace>W.Net</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.3\Tungsten.Net.XML</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="GenericClient.cs" />
    <Compile Remove="GenericServer.cs" />
    <Compile Remove="SampleUse.cs" />
    <Compile Remove="SecureStringClient.cs" />
    <Compile Remove="SecureStringServer.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="ExtensionMethods.cs" />
    <None Include="SerializationMethods.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="Tungsten" Version="2.0.0" />
  </ItemGroup>

</Project>