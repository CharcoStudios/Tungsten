<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <RootNamespace>W</RootNamespace>
    <Version>1.2.3</Version>
    <Authors>Jordan Duerksen</Authors>
    <Company />
    <Product>Tungsten</Product>
    <Description>Tungsten is a C# library of classes which make application development easier</Description>
    <Copyright>Copyright @ 2017 Jordan Duerksen</Copyright>
    <PackageProjectUrl>https://github.com/mode51/Tungsten</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/mode51/Tungsten/master/Src/Tungsten/Tungsten.ico</PackageIconUrl>
    <PackageTags>Tungsten.NuGet Tungsten W C# library</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <AssemblyVersion>1.2.3.0</AssemblyVersion>
    <FileVersion>1.2.3.0</FileVersion>
    <PackageReleaseNotes>Each Tungsten target now has it's own NuGet package</PackageReleaseNotes>
    <PostBuildEvent></PostBuildEvent>
    <RepositoryUrl>$(SolutionDir)NuGet</RepositoryUrl>
    <AssemblyName>Tungsten.Standard</AssemblyName>
    <PackageId>Tungsten.Standard</PackageId>
    <ApplicationIcon>Tungsten.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.3\Tungsten.Standard.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.3\Tungsten.Standard.xml</DocumentationFile>
    <OutputPath>bin\Release\netstandard1.3\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Runtime.Serialization.Json" Version="4.3.0" />
    <PackageReference Include="System.Runtime.Serialization.Xml" Version="4.3.0" />
    <PackageReference Include="System.Threading.Thread" Version="4.3.0" />
  </ItemGroup>

</Project>